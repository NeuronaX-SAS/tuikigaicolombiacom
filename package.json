{
  "name": "tuikigai",
  "version": "0.1.0",
  "description": "Aplicación de una sola página para explorar el concepto Ikigai",
  "scripts": {
    "build": "vite build",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "patch:worker": "node scripts/fix-worker.js",
    "copy:assets": "node scripts/copy-assets.js",
    "build.cloudflare": "npm run build && npm run build.server && npm run patch:worker && npm run copy:assets",
    "deploy": "wrangler pages deploy ./dist --project-name tuikigaicolombiacom",
    "dev": "vite --force",
    "preview": "vite preview",
    "serve": "wrangler pages dev ./dist --compatibility-flags=nodejs_als",
    "clean": "rimraf dist .turbo node_modules/.vite"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.5.7",
    "@builder.io/qwik-city": "^1.5.7",
    "axios": "^1.8.4",
    "canvas-confetti": "^1.6.0",
    "d3": "^7.8.5",
    "firebase": "^11.6.1",
    "framer-motion": "^10.16.4",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4",
    "ignore": "^7.0.3",
    "mercadopago": "^2.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230821.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@types/d3": "^7.4.0",
    "@types/node": "^20.17.30",
    "autoprefixer": "^10.4.14",
    "http-proxy-middleware": "^3.0.5",
    "postcss": "^8.4.27",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.0",
    "typescript": "^5.1.6",
    "vite": "^5.4.18",
    "wrangler": "^3.0.0"
  }
}
